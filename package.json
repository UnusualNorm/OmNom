{
  "name": "src",
  "version": "1.0.0",
  "description": "My not-so general purpose Discord bot, OmNom!",
  "main": "index.js",
  "repository": "github:UnusualNorm/OmNom",
  "scripts": {
    "build": "tsc",
    "dev:bridge": "npm run build && npm run start:bridge",
    "dev.debug:bridge": "npm run build && node --inspect-brk dist/bridge.js",
    "dev:client": "npm run build && npm run start:client",
    "dev.debug:client": "npm run build && node --inspect-brk dist/client.js",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint",
    "lint.fix": "eslint --fix",
    "start:bridge": "node dist/bridge.js",
    "start:client": "node dist/client.js"
  },
  "keywords": [],
  "author": "Unusual Norm",
  "license": "MIT",
  "dependencies": {
    "@sapphire/framework": "^3.1.3",
    "discord-cross-hosting": "^2.3.2",
    "discord-hybrid-sharding": "^1.7.4",
    "discord.js": "13.x",
    "dotenv-defaults": "^5.0.2",
    "validatorjs": "^3.22.1"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.1",
    "@types/dotenv-defaults": "^2.0.1",
    "@types/node": "^18.11.5",
    "@types/validatorjs": "^3.15.0",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4"
  }
}
